variable adc-base-username {
  description = "ADC username"
  type        = string
  default     = "nsroot"
}

variable adc-base-password {
  description = "ADC password"
  type        = string
  default     = ""
}

variable adc-base-ip-mgmt-address {
  description = "ADC mgmt IP address"
  type        = string
  default       = ""
}

variable adc-lic-filename {
  description = "ADC licensefile filename"
  type        = string
  default     = ""
}

variable adc-lic-filelocation {
  description = "ADC licensefile filelocation"
  type        = string
  default     = ""
}

variable adc-lic-filecontent {
  description = "ADC licensefile filecontent"
  type        = string
  default     = ""
}